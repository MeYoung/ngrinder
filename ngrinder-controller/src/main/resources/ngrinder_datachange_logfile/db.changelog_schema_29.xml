<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
				   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
	<changeSet author="ngrinder.3.3.1" id="29" dbms="h2, cubrid" onValidationFail="MARK_RAN">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="SITEMON_RESULT" />
			</not>
		</preConditions>
		<comment>Add table SITEMON_RESULT for sitemonitoring result data.</comment>

		<createTable tableName="SITEMON_RESULT">
			<column name="sitemon_id" type="varchar(50)">
				<constraints nullable="false" />
			</column>
			<column name="test_number" type="INTEGER">
				<constraints nullable="false" />
			</column>
			<column name="success" type="BIGINT" />
			<column name="error" type="BIGINT" />
			<column name="test_time" type="BIGINT" />
			<column name="time_stamp" type="TIMESTAMP">
				<constraints nullable="false" />
			</column>
		</createTable>
		<addPrimaryKey tableName="SITEMON_RESULT" columnNames="sitemon_id, test_number, time_stamp" />
	</changeSet>
</databaseChangeLog>